author = {Mastan, Erlita and Zhu, Shiping},
doi = {10.1021/acs.macromol.5b01525},
file = {:C$\backslash$:/Users/Raphael/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mastan, Zhu - 2015 - A Molecular Weight Distribution Polydispersity Equation for the ATRP System Quantifying the Effect of Radical Termi.pdf:pdf},
