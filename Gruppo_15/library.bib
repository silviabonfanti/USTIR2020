issn = {10990488},
journal = {Journal of Polymer Science, Part B: Polymer Physics},
keywords = {3D printing filaments,annealing,cellulose nanofibers,extrusion,mechanical properties},
number = {11},
pages = {847--855},
title = {{The influence of grafted cellulose nanofibers and postextrusion annealing treatment on selected properties of poly(lactic acid) filaments for 3D printing}},
