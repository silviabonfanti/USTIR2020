keywords = {Curing behavior,Soybean oil,Structural plastic,Synergetic modification,Thermosetting polymer,Unsaturated ester,Unsaturated polyester resin (UPR)},
number = {8},
pages = {4208--4216},
title = {{Highly Functional Unsaturated Ester Macromonomer Derived from Soybean Oil: Synthesis and Copolymerization with Styrene}},
