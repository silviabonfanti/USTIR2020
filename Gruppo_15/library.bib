author = {Kowalski, Adam and Libiszowski, Jan and Biela, Tadeusz and Cypryk, Marek and Duda, Andrzej and Penczek, Stanislaw},
doi = {10.1021/ma050752j},
file = {:C$\backslash$:/Users/Raphael/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kowalski et al. - 2005 - Kinetics and Mechanism of Cyclic Esters Polymerization Initiated with Tin ( II ) Octoate . Polymerization of �.pdf:pdf},
issn = {0024-9297},
journal = {Society},
number = {Ii},
pages = {8170--8176},
title = {{Kinetics and Mechanism of Cyclic Esters Polymerization Initiated with Tin ( II ) Octoate . Polymerization of e -Caprolactone and l , l-Lactide Co-initiated with Primary Amines Co-initiated with Primary Amines}},
year = {2005}
