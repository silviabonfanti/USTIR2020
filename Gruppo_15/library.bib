journal = {Journal of Applied Polymer Science},
keywords = {mechanical properties,nanostructured polymers,structure–property relationships,thermoplastics extrusion},
number = {7},
pages = {1--9},
title = {{Effect of nanoparticles on the mechanical properties of acrylonitrile–butadiene–styrene specimens fabricated by fused deposition modeling}},
volume = {134},
year = {2017}
}
