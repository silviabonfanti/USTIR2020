pages = {421--454},
publisher = {Wiley-Blackwell},
title = {{Modelling free-radical copolymerization kinetics—evaluation of the pseudo-kinetic rate constant method, 1. Molecular weight calculations for linear copolymers}},
url = {https://doi.org/10.1002/mats.1993.040020311},
volume = {2},
year = {2018}
}
