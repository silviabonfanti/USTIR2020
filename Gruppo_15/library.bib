publisher = {Wiley-Blackwell},
title = {{Calculation of molecular weight distributions in free-radical polymerization with chain branching}},
url = {https://doi.org/10.1002/(SICI)1521-3919(19990901)8:5{\%}3C498::AID-MATS498{\%}3E3.0.CO http://2-5},
volume = {8},
year = {1999}
}
@article{VanAlstine2018,
abstract = {The most common alternative separation methods used in bioprocessing are flocculation and precipitation. They are proven, readily scaled, do not require overt energy input or capital investment, and can offer significant selectivity. They have the ability to handle modern challenges such as high-density feeds that tax the limits of industry "workhorse" methods such as centrifugation, filtration, and chromatography. They appear particularly well suited to be used in the initial stages of bioprocessing, either in target-capture or contaminant-removal. Such methods are compatible with modern high-throughput screening of operating conditions, as well as single-use equipment for use in batch or continuous processes. This chapter provides more insight into a discussion of flocculation and precipitation, initiated in the preceding chapter.},
