isbn = {0959-8103},
issn = {09598103},
keywords = {Comonomer sequencing randomized,Diphenylzinc,L-lactide,Transesterification,$\epsilon$-caprolactone},
number = {9},
pages = {1049--1056},
title = {{Ring-opening copolymerization of L-lactide with $\epsilon$-caprolactone initiated by diphenylzinc}},
volume = {55},
