journal = {Materials},
keywords = {Biocomposites,Epoxidized soybean oil,Hydrophobicity,Interface bonding,Screw extrusion steam explosion},
number = {9},
title = {{Preparation and characteristics of biocomposites based on steam exploded sisal fiber modified with amphipathic epoxidized soybean oil resin}},
