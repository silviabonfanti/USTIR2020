file = {:C$\backslash$:/Users/Raphael/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dubey et al. - 2016 - Modelling and Validation of Synthesis of Poly Lactic Acid Using an Alternative Energy Source through a Continuous.pdf:pdf},
isbn = {4401234758},
issn = {2073-4360},
journal = {Polymers},
keywords = {alternative energy,bio-degradable,mathematical,metal catalyst,modelling,pla,poly lactic acid,reactive extrusion,ring opening polymerization,rop},
number = {4},
pages = {164},
title = {{Modelling and Validation of Synthesis of Poly Lactic Acid Using an Alternative Energy Source through a Continuous Reactive Extrusion Process}},
url = {http://www.mdpi.com/2073-4360/8/4/164},
volume = {8},
year = {2016}
}
