keywords = {Bone and tissue engineering,Drug and gene delivery,Food and pharmaceutical industry,Pullulan,Water treatment},
pages = {603--632},
publisher = {Elsevier B.V.},
title = {{A review on versatile applications of blends and composites of pullulan with natural and synthetic polymers}},
