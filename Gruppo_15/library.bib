journal = {Chemosphere},
keywords = {Cellulose nanocrystals (CNCs),Cellulose nanofibers (CNFs),Membrane fouling,Surface coating,Ultrafiltration},
pages = {76--84},
publisher = {Elsevier Ltd},
title = {{Surface coating of UF membranes to improve antifouling properties: A comparison study between cellulose nanocrystals (CNCs) and cellulose nanofibrils (CNFs)}},
