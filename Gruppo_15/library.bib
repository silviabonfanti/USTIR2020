file = {:C$\backslash$:/Users/Raphael/Documents/Alana/Universidades Alemanha/TUHH/Tese/Kinetic modeling of the oxidative dehydrogenation of ethane to ethylene over a MoVTeNbO catalytic system.pdf:pdf},
issn = {13858947},
journal = {Chemical Engineering Journal},
keywords = {Ethylene,Kinetic modeling,Langmuir-Hinshelwood-Hougen-Watson,Mars-van Krevelen,MoVTeNbO catalyst,Oxidative dehydrogenation reaction},
pages = {75--88},
publisher = {Elsevier B.V.},
title = {{Kinetic modeling of the oxidative dehydrogenation of ethane to ethylene over a MoVTeNbO catalytic system}},
url = {http://dx.doi.org/10.1016/j.cej.2014.04.042},
volume = {252},
year = {2014}
