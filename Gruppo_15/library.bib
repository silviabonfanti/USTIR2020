keywords = {Battery,Conductive materials,Energy device,Nanocellulose,Solar cell,Supercapacitor},
number = {March},
pages = {299--320},
title = {{Nanocellulose-based conductive materials and their emerging applications in energy devices - A review}},
volume = {35},
year = {2017}
}
@misc{Gustafsson2015,
abstract = {Poly($\epsilon$-caprolactone) (PCL) is a ductile, bioabsorbable polymer that has been employed as a blend partner for poly(L-lactic acid) (PLLA). An improvement of the material strength and impact resistance of PLLA/PCL polymer blends compared to pure PLLA has been shown previously. To use numerical simulations in the design process of new components composed of the PLLA/PCL blend, a constitutive model for the material has to be established. In this work, a constitutive model for a PLLA/PCL polymer blend is established from the results of compressive tests at high and low strain rates at three different temperatures, including the body temperature. Finite element simulations of the split Hopkinson pressure bar test using the established constitutive model are carried out under the same condition as the experiments. During the experiments, the changes in the diameter and thickness of the specimens are captured by a high-speed video camera. The accuracy of the numerical model is tested by comparing the simulation results, such as the stress, strain, thickness and diameter histories of the specimens, with those measured in the experiments. The numerical model is also validated against an impact test of non-homogenous strains and strain rates. The results of this study provide a validated numerical model for a PLLA/PCL polymer blend at strain rates of up to 1800s{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} in the temperature range between 22°C and 50°C.},
author = {Gustafsson, Gustaf and Nishida, Masahiro and Ito, Yoshitaka and H{\"{a}}ggblad, Hans {\AA}ke and Jons{\'{e}}n, P{\"{a}}r and Takayama, Tetsuo and Todo, Mitsugu},
booktitle = {Journal of the Mechanical Behavior of Biomedical Materials},
doi = {10.1016/j.jmbbm.2015.07.007},
file = {:C$\backslash$:/Users/Raphael/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gustafsson et al. - 2015 - Mechanical characterization and modelling of the temperature-dependent impact behaviour of a biocompatible po.pdf:pdf},
issn = {18780180},
keywords = {Constitutive model,Finite element method,PCL,PLLA,Split Hopkinson pressure bar,Temperature dependency},
pages = {279--290},
pmid = {26275490},
title = {{Mechanical characterization and modelling of the temperature-dependent impact behaviour of a biocompatible poly(L-lactide)/poly($\epsilon$-caprolactone) polymer blend}},
volume = {51},
year = {2015}
}
@article{Wang2019,
