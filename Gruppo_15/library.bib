keywords = {Blockchain,Business processes,Consensus,Service,Smart contract,Specification language,System Architecture,Workflow},
number = {July 2018},
pages = {32--39},
publisher = {Elsevier},
title = {{Blockchain characteristics and consensus in modern business processes}},
url = {https://doi.org/10.1016/j.jii.2018.07.004},
volume = {13},
year = {2018}
}
